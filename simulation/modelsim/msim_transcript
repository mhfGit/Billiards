vsim work.vga_controller2
# vsim work.vga_controller2 
# Loading sv_std.std
# Loading work.vga_controller2
wave create -driver freeze -pattern clock -initialvalue HiZ -period 20ns -dutycycle 50 -starttime 0ns -endtime 1000ns sim:/vga_controller2/Clk
wave create -driver freeze -pattern constant -value 0 -starttime 0ns -endtime 1000ns sim:/vga_controller2/Reset
wave edit change_value -start 4716ps -end 33010ps -value 1 Edit:/vga_controller2/Reset
add wave -position end  sim:/vga_controller2/DrawX
add wave -position end  sim:/vga_controller2/DrawY
run -all
